{
    "name": "Chrome Extension React Template",
    "description": "A simple chrome browser extension template using React.",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "./16.png",
        "32": "./32.png",
        "48": "./48.png",
        "128": "./128.png"
    },
    "background": {
        "service_worker": "./background.js"
    },
    "options_page": "./options.html",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}
